{
  "extends": "@acme/tsconfig/internal-package.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "ES2022"
    ],
    "jsx": "preserve",
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "baseUrl": ".",
    "paths": {
      "@acme/ui": [
        "src"
      ],
      "@acme/ui/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "*.ts",
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
